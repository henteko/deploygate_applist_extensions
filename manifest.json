{
    "name": "deploygateアプリ一覧",
        "version": "0.0.1",
        "manifest_version": 2,
        "description": "deploygateに登録してあるアプリ一覧が見れます",
        "permissions": [
            "https://deploygate.com/*",
            "alarms"
         ],
        "browser_action": {
            "default_icon": "icon.png",
            "default_title": "deploygate applist",
            "default_popup":"popup.html"
        }
}
